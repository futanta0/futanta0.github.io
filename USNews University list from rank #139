            { rank: '#373', name: 'University of Louisiana at Lafayette', state: 'LA', location: 'Lafayette' },
            { rank: '#373', name: 'University of West Georgia', state: 'GA', location: 'Carrollton' },
            { rank: '#373', name: 'William Carey University', state: 'MS', location: 'Hattiesburg' },
            { rank: '#373', name: 'Wingate University', state: 'NC', location: 'Wingate' },
            { rank: '#384', name: 'Baker University', state: 'KS', location: 'Baldwin City' },
            { rank: '#384', name: 'Briar Cliff University', state: 'IA', location: 'Sioux City' },
            { rank: '#384', name: 'Capitol Technology University', state: 'MD', location: 'Laurel' }
        ];

        let filteredData = [...universityData];
        let currentPage = 1;
        let rowsPerPage = 15;

        const tableBody = document.getElementById('universityTableBody');
        const searchInput = document.getElementById('searchInput');
        const paginationContainer = document.getElementById('pagination');
        const noResultsDiv = document.getElementById('noResults');
        const rowsPerPageSelect = document.getElementById('rowsPerPageSelect');


        function generateSearchUrl(universityName) {
            const query = encodeURIComponent(`${universityName} graduate programs`);
            return `https://www.google.com/search?q=${query}`;
        }

        function renderTable() {
            tableBody.innerHTML = '';
            
            if (filteredData.length === 0) {
                noResultsDiv.classList.remove('hidden');
                tableBody.parentElement.parentElement.classList.add('hidden');
                return;
            }

            noResultsDiv.classList.add('hidden');
            tableBody.parentElement.parentElement.classList.remove('hidden');

            const start = (currentPage - 1) * rowsPerPage;
            const end = start + rowsPerPage;
            const paginatedItems = filteredData.slice(start, end);

            for (const university of paginatedItems) {
                const row = document.createElement('tr');
                row.className = 'hover:bg-gray-50';
                
                const gradProgramUrl = generateSearchUrl(university.name);

                row.innerHTML = `
                    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">${university.rank}</td>
                    <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-800">${university.name}</td>
                    <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">${university.location}</td>
                    <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">${university.state}</td>
                    <td class="px-6 py-4 whitespace-nowrap text-sm text-blue-600">
                        <a href="${gradProgramUrl}" target="_blank" rel="noopener noreferrer" class="hover:underline">
                            Search Website
                            <svg class="w-4 h-4 inline-block ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"></path></svg>
                        </a>
                    </td>
                `;
                tableBody.appendChild(row);
            }
        }

        function renderPagination() {
            paginationContainer.innerHTML = '';
            const pageCount = Math.ceil(filteredData.length / rowsPerPage);
            
            if(pageCount <= 1) return;

            const startEntry = (currentPage - 1) * rowsPerPage + 1;
            const endEntry = Math.min(currentPage * rowsPerPage, filteredData.length);

            let summaryHTML = `<div class="hidden sm:block">
                <p class="text-sm text-gray-700">
                    Showing
                    <span class="font-medium">${startEntry}</span>
                    to
                    <span class="font-medium">${endEntry}</span>
                    of
                    <span class="font-medium">${filteredData.length}</span>
                    results
                </p>
            </div>`;

            let buttonsHTML = `<div class="flex-1 flex justify-between sm:justify-end">`;
            
            if (currentPage > 1) {
                 buttonsHTML += `<button onclick="changePage(${currentPage - 1})" class="relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50">Previous</button>`;
            } else {
                 buttonsHTML += `<button class="relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-400 bg-white cursor-not-allowed" disabled>Previous</button>`;
            }

            if (currentPage < pageCount) {
                buttonsHTML += `<button onclick="changePage(${currentPage + 1})" class="ml-3 relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50">Next</button>`;
            } else {
                buttonsHTML += `<button class="ml-3 relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-400 bg-white cursor-not-allowed" disabled>Next</button>`;
            }
            buttonsHTML += `</div>`;

            paginationContainer.innerHTML = summaryHTML + buttonsHTML;
        }

        function changePage(page) {
            currentPage = page;
            renderTable();
            renderPagination();
            window.scrollTo(0,0);
        }

        searchInput.addEventListener('keyup', (e) => {
            const searchTerm = e.target.value.toLowerCase();
            filteredData = universityData.filter(uni => {
                return (
                    uni.name.toLowerCase().includes(searchTerm) ||
                    uni.state.toLowerCase().includes(searchTerm) ||
                    (uni.location && uni.location.toLowerCase().includes(searchTerm))
                );
            });
            currentPage = 1;
            renderTable();
            renderPagination();
        });

        rowsPerPageSelect.addEventListener('change', (e) => {
            rowsPerPage = parseInt(e.target.value, 10);
            currentPage = 1;
            renderTable();
            renderPagination();
        });


        document.addEventListener('DOMContentLoaded', () => {
            rowsPerPage = parseInt(rowsPerPageSelect.value, 10);
            renderTable();
            renderPagination();
        });
    </script>

</body>
</html>

